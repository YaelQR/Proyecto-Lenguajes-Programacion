<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main"
    android:background="#2567E8"
    tools:context=".LogininicialActivity">

    <TextView
        android:id="@+id/titulo"
        android:layout_width="205dp"
        android:layout_height="49dp"
        android:layout_marginBottom="72dp"
        android:text="Registrate"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FAF7F7"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cuadroBlanco3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/cuadroBlanco"
        android:layout_width="422dp"
        android:layout_height="494dp"
        android:layout_marginTop="424dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/cuadroBlanco3"
        android:layout_width="303dp"
        android:layout_height="401dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/redondeado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/titulo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonRegistro"
        android:layout_width="172dp"
        android:layout_height="56dp"
        android:layout_marginBottom="16dp"
        android:text="@string/registrarse"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:backgroundTint="#2567E8"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/cuadroBlanco3"
        app:layout_constraintEnd_toEndOf="@+id/cuadroBlanco3"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/cuadroBlanco3" />

    <EditText
        android:id="@+id/emailRegister"
        android:layout_width="250dp"
        android:layout_height="57dp"
        android:layout_marginBottom="16dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/email"
        android:inputType="text"
        android:textColor="@android:color/black"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/txtTelefono"
        app:layout_constraintEnd_toEndOf="@+id/cuadroBlanco3"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/cuadroBlanco3" />

    <EditText
        android:id="@+id/pswdRegister"
        android:layout_width="250dp"
        android:layout_height="57dp"
        android:layout_marginBottom="32dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/contrase_a"
        android:inputType="textPassword"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/buttonRegistro"
        app:layout_constraintEnd_toEndOf="@+id/cuadroBlanco3"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/cuadroBlanco3" />

    <EditText
        android:id="@+id/txtTelefono"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:hint="TelÃ©fono "
        android:inputType="phone"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/pswdRegister"
        app:layout_constraintEnd_toEndOf="@+id/cuadroBlanco3"
        app:layout_constraintStart_toStartOf="@+id/cuadroBlanco3" />

    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="115dp"
        android:layout_height="47dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/emailRegister"
        app:layout_constraintEnd_toStartOf="@+id/txtApellido"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toStartOf="@+id/cuadroBlanco3"
        app:layout_constraintTop_toTopOf="@+id/cuadroBlanco3"
        app:layout_constraintVertical_bias="0.745" />

    <EditText
        android:id="@+id/txtApellido"
        android:layout_width="115dp"
        android:layout_height="47dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Apellido"
        android:inputType="text"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/emailRegister"
        app:layout_constraintEnd_toEndOf="@+id/cuadroBlanco3"
        app:layout_constraintTop_toTopOf="@+id/cuadroBlanco3"
        app:layout_constraintVertical_bias="0.739" />

</androidx.constraintlayout.widget.ConstraintLayout>
